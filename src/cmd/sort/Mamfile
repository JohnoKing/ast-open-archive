done main.c
meta main.o %.c>%.o main.c main
prev main.c
exec - ${CC} ${mam_cc_FLAGS} ${CCFLAGS} -I${PACKAGE_ast_INCLUDE} -D_PACKAGE_ast -DUSAGE_LICENSE=\""[-author?Glenn Fowler <gsf@research.att.com>][-author?Phong Vo <kpv@research.att.com>][-author?Doug McIlroy <doug@research.bell-labs.com>][-copyright?Copyright (c) 1996-2006 AT&T Corp.][-license?http://www.opensource.org/licenses/cpl1.0.txt][--catalog?sort]"\" -c main.c
done main.o generated
make sfopen.o
make sfopen.c
prev ${PACKAGE_ast_INCLUDE}/error.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast.h implicit
done sfopen.c
meta sfopen.o %.c>%.o sfopen.c sfopen
prev sfopen.c
setv LICENSE -DLICENSE="since=2005,author=gsf"
exec - ${CC} ${mam_cc_FLAGS} ${CCFLAGS} -I${PACKAGE_ast_INCLUDE} -D_PACKAGE_ast -c sfopen.c
done sfopen.o generated
bind -lrecsort
bind -ldll
bind -lz
bind -last
exec - ${CC} ${CCLDFLAGS} ${mam_cc_FLAGS} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${INSTALLROOT}/lib} -o sort main.o sfopen.o ${mam_librecsort} ${mam_libdll} ${mam_libz} ${mam_libast}
done sort generated
make ${INSTALLROOT}/bin
exec - if	silent test ! -d ${INSTALLROOT}/bin
exec - then	mkdir -p ${INSTALLROOT}/bin
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/sort
prev ${INSTALLROOT}/bin
prev sort
exec - test '' = 'sort' || ${STDCMP} 2>/dev/null -s sort ${INSTALLROOT}/bin/sort || { ${STDMV} ${INSTALLROOT}/bin/sort ${INSTALLROOT}/bin/sort.old 2>/dev/null || true; ${STDCP} sort ${INSTALLROOT}/bin/sort  		    		    ;}
done ${INSTALLROOT}/bin/sort generated
make ${INSTALLROOT}/man/man1
exec - if	silent test ! -d ${INSTALLROOT}/man/man1
exec - then	mkdir -p ${INSTALLROOT}/man/man1
exec - fi
done ${INSTALLROOT}/man/man1 generated
make ${INSTALLROOT}/man/man1/sort.1
prev ${INSTALLROOT}/man/man1
make sort.1
done sort.1
exec - test '' = 'sort.1' || ${STDCMP} 2>/dev/null -s sort.1 ${INSTALLROOT}/man/man1/sort.1 || { ${STDMV} ${INSTALLROOT}/man/man1/sort.1 ${INSTALLROOT}/man/man1/sort.1.old 2>/dev/null || true; ${STDCP} sort.1 ${INSTALLROOT}/man/man1/sort.1  		    		    ;}
done ${INSTALLROOT}/man/man1/sort.1 generated
done install virtual
make test
make test.sort
make sort.tst
done sort.tst
prev sort
exec - regress  sort.tst sort
done test.sort virtual
make test.script
make testsort
make testsort.sh
done testsort.sh
meta testsort %.sh>% testsort.sh testsort
prev testsort.sh
exec - case static,port:$OPTIND:$RANDOM in
exec - ?*:*:*|*::*|*:*:$RANDOM)
exec - 	;;
exec - *)	if	ENV= x= $SHELL -nc ': ${list[level]}  !(pattern)' 2>/dev/null
exec - 	then	ENV= $SHELL -n testsort.sh
exec - 	fi
exec - 	;;
exec - esac
exec - case '${mam_cc_SHELLMAGIC}' in
exec - "")	case 14 in
exec - 	0)	cp testsort.sh testsort
exec - 		;;
exec - 	*)	{
exec - 		i=`(read x; echo $x) < testsort.sh`
exec - 		case $i in
exec - 		'#!'*|*'||'*|':'*|'":"'*|"':'"*)	echo "$i" ;;
exec - 		esac
exec - 		cat - testsort.sh <<'!'
exec - TESTCC='${CC}'
exec - !
exec - 		} > testsort
exec - 		;;
exec - 	esac
exec - 	;;
exec - *)	cat - testsort.sh > testsort <<'!'
exec - ${mam_cc_SHELLMAGIC}
exec - TESTCC='${CC}'
exec - !
exec - 	;;
exec - esac
exec - silent test -w testsort -a -x testsort || chmod u+w,+x testsort
done testsort generated
exec - testsort sort
done test.script virtual
done test dontcare virtual
