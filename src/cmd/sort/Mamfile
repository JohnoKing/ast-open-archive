done main.o generated
setv mam_librecsort ${mam_cc_L+-lrecsort}${mam_cc_L-${PACKAGE_ast_LIB}/librecsort.a}
setv mam_librecsort `${mam_cc_L+cat} ${mam_cc_L-sed} ${mam_cc_L-s,-lrecsort,${INSTALLROOT}/lib/librecsort.a,} ${PACKAGE_ast_LIB}/lib/recsort`
setv mam_libz ${mam_cc_L+-lz}${mam_cc_L-${PACKAGE_ast_LIB}/libz.a}
setv mam_libz `${mam_cc_L+cat} ${mam_cc_L-sed} ${mam_cc_L-s,-lz,${INSTALLROOT}/lib/libz.a,} ${PACKAGE_ast_LIB}/lib/z`
setv mam_libast ${mam_cc_L+-last}${mam_cc_L-${PACKAGE_ast_LIB}/libast.a}
exec - ${CC} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${PACKAGE_ast_LIB}}  -o sort main.o ${mam_librecsort} ${mam_libz} ${mam_libast}
done sort generated
make ${INSTALLROOT}/bin
exec - set -
exec - if	test ! -d ${INSTALLROOT}/bin
exec - then	set -x && mkdir -p ${INSTALLROOT}/bin 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${INSTALLROOT}/bin | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/sort
prev sort
exec - mv ${INSTALLROOT}/bin/sort ${INSTALLROOT}/bin/sort.old 2>/dev/null || true; cp sort ${INSTALLROOT}/bin/sort
done ${INSTALLROOT}/bin/sort generated
make ${INSTALLROOT}/man/man1
exec - set -
exec - if	test ! -d ${INSTALLROOT}/man/man1
exec - then	set -x && mkdir -p ${INSTALLROOT}/man/man1 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${INSTALLROOT}/man/man1 | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${INSTALLROOT}/man/man1 generated
make ${INSTALLROOT}/man/man1/sort.1
make sort.1
done sort.1
exec - mv ${INSTALLROOT}/man/man1/sort.1 ${INSTALLROOT}/man/man1/sort.1.old 2>/dev/null || true; cp sort.1 ${INSTALLROOT}/man/man1/sort.1
done ${INSTALLROOT}/man/man1/sort.1 generated
done install virtual
make test
make test.sort
make sort.tst
done sort.tst
prev sort
exec - regress  sort.tst sort
done test.sort virtual
make test.script
make testsort
make testsort.sh
done testsort.sh
prev testsort.sh
exec - case static,port:$OPTIND:$RANDOM in
exec - ?*:*:*|*::*|*:*:$RANDOM)
exec - 	;;
exec - *)	if	ENV= x= $SHELL -n -c '${list[level]} ' 2>/dev/null
exec - 	then	ENV= $SHELL -n testsort.sh
exec - 	fi
exec - 	;;
exec - esac
exec - case '' in
exec - "")	cp testsort.sh testsort
exec - 	;;
exec - *)	{
exec - 	i=`(read x; echo $x) < testsort.sh`
exec - 	case $i in
exec - 	'#!'*|*'||'*|':'*|'":"'*|"':'"*)	echo $i ;;
exec - 	esac
exec - 	echo 
exec - 	cat testsort.sh
exec - 	} > testsort
exec - 	;;
exec - esac
exec - chmod u+w,+x testsort
done testsort generated
exec - testsort sort
done test.script virtual
done test dontcare virtual
