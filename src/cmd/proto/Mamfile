meta sear %.sh>% sear.sh sear
prev sear.sh
setv LICENSE -DLICENSE="since=2001,author=dgk+gsf"
exec - case static,port:$OPTIND:$RANDOM in
exec - ?*:*:*|*::*|*:*:$RANDOM)
exec - 	;;
exec - *)	if	ENV= LC_ALL=C x= $SHELL -nc ': ${list[level]}  !(pattern)' 2>/dev/null
exec - 	then	ENV= LC_ALL=C $SHELL -n sear.sh
exec - 	fi
exec - 	;;
exec - esac
exec - case '${mam_cc_SHELLMAGIC}' in
exec - "")	case 240 in
exec - 	0)	cp sear.sh sear
exec - 		;;
exec - 	*)	{
exec - 		i=`(read x; echo $x) < sear.sh`
exec - 		case $i in
exec - 		'#!'*|*'||'*|':'*|'":"'*|"':'"*)	echo "$i" ;;
exec - 		esac
exec - 		cat - sear.sh <<'!'
exec - USAGE_LICENSE="[-author?David Korn <dgk@research.att.com>][-author?Glenn Fowler <gsf@research.att.com>][-copyright?Copyright (c) 2001-2010 AT&T Intellectual Property][-license?http://www.opensource.org/licenses/cpl1.0.txt][--catalog?proto]"
exec - !
exec - 		} > sear
exec - 		;;
exec - 	esac
exec - 	;;
exec - *)	cat - sear.sh > sear <<'!'
exec - ${mam_cc_SHELLMAGIC}
exec - USAGE_LICENSE="[-author?David Korn <dgk@research.att.com>][-author?Glenn Fowler <gsf@research.att.com>][-copyright?Copyright (c) 2001-2010 AT&T Intellectual Property][-license?http://www.opensource.org/licenses/cpl1.0.txt][--catalog?proto]"
exec - !
exec - 	;;
exec - esac
exec - silent test -w sear -a -x sear || chmod u+w,+x sear
done sear generated
