setv INSTALLROOT ${INSTALLROOT}
setv PACKAGE_ast ${INSTALLROOT}
setv PACKAGE_ast_INCLUDE ${PACKAGE_ast}/include/ast
setv PACKAGE_ast_LIB ${PACKAGE_ast}/lib
setv PACKAGEROOT ${PACKAGEROOT}
setv AR ar
setv ARFLAGS cr
setv AS as
setv ASFLAGS
setv CC cc
setv mam_cc_FLAGS ${mam_cc_DLL}
setv CCFLAGS ${-debug-symbols?1?${mam_cc_DEBUG} -D_BLD_DEBUG?${mam_cc_OPTIMIZE}?}
setv CCLDFLAGS  ${-strip-symbols?1?${mam_cc_LD_STRIP}??}
setv COTEMP $$
setv CPIO cpio
setv CPIOFLAGS
setv CPP "${CC} -E"
setv F77 f77
setv HOSTCC ${CC}
setv IGNORE
setv LD ld
setv LDFLAGS
setv LEX lex
setv LEXFLAGS
setv LPR lpr
setv LPRFLAGS
setv M4FLAGS
setv NMAKE nmake
setv NMAKEFLAGS
setv PR pr
setv PRFLAGS
setv SHELL /bin/sh
setv SILENT
setv TAR tar
setv YACC yacc
setv YACCFLAGS -d
make ${INSTALLROOT}/lib/package/ast.lic
done ${INSTALLROOT}/lib/package/ast.lic
make install
make edit
make libedit.a archive
make edit.req
exec - set -
exec - echo 'int main(){return 0;}' > 1.${COTEMP}.c
exec - ${CC} ${mam_cc_FLAGS} ${CCFLAGS}   -c 1.${COTEMP}.c &&
exec - x=`${CC} ${mam_cc_FLAGS} ${CCFLAGS}   ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o -l'*' 2>&1 | sed -e 's/[][()+@?]/#/g' || :` &&
exec - {
exec - case "" in
exec - *?)	echo " " ;;
exec - esac
exec - for i in edit dll ast
exec - do	case $i in
exec - 	"edit"|edit)
exec - 		;;
exec - 	*)	if	test ! -f ${INSTALLROOT}/lib/lib$i.a
exec - 		then	case `{ ${CC} ${mam_cc_FLAGS} ${CCFLAGS}    ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 			*$x*)	case `{ ${CC} ${mam_cc_FLAGS} ${CCFLAGS}   ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 				*$x*) continue ;;
exec - 				esac
exec - 				;;
exec - 			esac
exec - 		fi
exec - 		;;
exec - 	esac
exec - 	echo " -l$i"
exec - done
exec - } > edit.req
exec - rm -f 1.${COTEMP}.*
done edit.req generated
make editlib.o
make editlib.c
make ${INSTALLROOT}/include/ast/dlldefs.h implicit
done ${INSTALLROOT}/include/ast/dlldefs.h dontcare
make edit.h implicit
make national.h implicit
done national.h dontcare virtual
done edit.h
make history.h implicit
done history.h
make terminal.h implicit
done terminal.h
make io.h implicit
make ${INSTALLROOT}/include/ast/re_comp.h implicit
make ${INSTALLROOT}/include/ast/prototyped.h implicit
make ${INSTALLROOT}/include/prototyped.h implicit
done ${INSTALLROOT}/include/prototyped.h dontcare
done ${INSTALLROOT}/include/ast/prototyped.h dontcare
done ${INSTALLROOT}/include/ast/re_comp.h dontcare
make ${INSTALLROOT}/include/ast/getopt.h implicit
make ${INSTALLROOT}/include/ast/ast_getopt.h implicit
prev ${INSTALLROOT}/include/ast/prototyped.h implicit
done ${INSTALLROOT}/include/ast/ast_getopt.h dontcare
prev ${INSTALLROOT}/include/ast/prototyped.h implicit
done ${INSTALLROOT}/include/ast/getopt.h dontcare
make error?--?old?ethernet?address?at?wrong?offset implicit
done error?--?old?ethernet?address?at?wrong?offset dontcare virtual
make sh_config.h implicit
make FEATURE/ielib implicit
meta FEATURE/ielib features/%>FEATURE/% features/ielib ielib
make features/ielib
done features/ielib
prev edit.req
bind -ldll
bind -last
exec - iffe -v -c '${CC} ${mam_cc_FLAGS} ${CCFLAGS}   ${LDFLAGS} '  ref ${mam_cc_L+-L${INSTALLROOT}/lib} -I${INSTALLROOT}/include/ast -I${INSTALLROOT}/include ${mam_libdll} ${mam_libast} ${mam_libast} : run features/ielib
done FEATURE/ielib dontcare generated
make FEATURE/options implicit
meta FEATURE/options features/%.sh>FEATURE/% features/options.sh options
make features/options.sh
make OPTIONS implicit
done OPTIONS
done features/options.sh dontcare
prev edit.req
exec - iffe -v -c '${CC} ${mam_cc_FLAGS} ${CCFLAGS}  -DLDYNAMIC -DVSH -DESH ${LDFLAGS} '  ref ${mam_cc_L+-L${INSTALLROOT}/lib} -I${INSTALLROOT}/include/ast -I${INSTALLROOT}/include ${mam_libdll} ${mam_libast} ${mam_libast} : run features/options.sh OPTIONS
done FEATURE/options dontcare generated
done sh_config.h
make ${INSTALLROOT}/include/ast/error.h implicit
