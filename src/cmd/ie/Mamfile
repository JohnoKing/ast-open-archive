info mam static 00000 1994-07-17 make (AT&T Research) 4.0 2000-02-01
setv INSTALLROOT ../../..
setv PACKAGE_ast ${INSTALLROOT}
setv PACKAGE_ast_INCLUDE ${PACKAGE_ast}/include
setv PACKAGE_ast_LIB ${PACKAGE_ast}/lib
setv PACKAGEROOT ../../../../..
setv AR ar
setv ARFLAGS cr
setv AS as
setv ASFLAGS
setv CC cc
setv CCFLAGS "-O ${mam_cc_DLL}"
setv COTEMP $$
setv CPIO cpio
setv CPIOFLAGS
setv CPP "${CC} -E"
setv F77 f77
setv HOSTCC ${CC}
setv IGNORE
setv LD ld
setv LDFLAGS 
setv LEX lex
setv LEXFLAGS
setv LPR lpr
setv LPRFLAGS
setv M4FLAGS 
setv MAKE nmake
setv MAKEFLAGS
setv PR pr
setv PRFLAGS
setv SHELL /bin/sh
setv SILENT
setv TAR tar
setv YACC yacc
setv YACCFLAGS -d
make ${PACKAGE_ast_LIB}/package/ast.lic
done ${PACKAGE_ast_LIB}/package/ast.lic
make install
make edit
make libedit.a archive
make edit.req
exec - echo 'main(){return(0);}' > 1.${COTEMP}.c
exec - ${CC} ${CCFLAGS}    -c 1.${COTEMP}.c &&
exec - x=`${CC} ${CCFLAGS}    ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o -l'*' 2>&1 | sed -e 's/[][()+@?]/#/g' || :` &&
exec - {
exec - case "" in
exec - *?)	echo " " ;;
exec - esac
exec - for i in edit dll ast
exec - do	case $i in
exec - 	"edit"|edit)
exec - 		;;
exec - 	*)	if	test ! -f ${INSTALLROOT}/lib/lib$i.a
exec - 		then	case `{ ${CC} ${CCFLAGS}    -L. -Lconfigure ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 			*$x*)	case `{ ${CC} ${CCFLAGS}    ${LDFLAGS}  -o 1.${COTEMP}.x 1.${COTEMP}.o  -l$i 2>&1 || echo '' $x ;} | sed -e 's/[][()+@?]/#/g' || :` in
exec - 				*$x*) continue ;;
exec - 				esac
exec - 				;;
exec - 			esac
exec - 		fi
exec - 		;;
exec - 	esac
exec - 	echo " -l$i"
exec - done
exec - } > edit.req
exec - rm -f 1.${COTEMP}.*
done edit.req generated
make editlib.o
make editlib.c
make ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/dlldefs.h dontcare
make edit.h implicit
make national.h implicit
done national.h dontcare virtual
done edit.h
make history.h implicit
done history.h
make terminal.h implicit
done terminal.h
make io.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/re_comp.h implicit
make ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/prototyped.h dontcare
done ${PACKAGE_ast_INCLUDE}/ast/re_comp.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/getopt.h dontcare
make error?--?old?ethernet?address?at?wrong?offset implicit
done error?--?old?ethernet?address?at?wrong?offset dontcare virtual
make sh_config.h implicit
make configure
done configure
make configure/config
done configure/config
exec - configure/config configure ${CC} > sh_config.tmp
exec - if	test -r sh_config.tmp && cmp -s sh_config.tmp sh_config.h
exec - then	rm sh_config.tmp
exec - else	mv sh_config.tmp sh_config.h
exec - fi
done sh_config.h generated
make ${PACKAGE_ast_INCLUDE}/ast/error.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/option.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/sfio.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_common.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_botch.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_botch.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_unistd.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_unistd.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_fcntl.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ls.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_mode.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_mode.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ls.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_fs.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_fcntl.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_types.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_types.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/stdio.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
prev ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/stdio.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/limits.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/limits.h dontcare
make ${PACKAGE_ast_INCLUDE}/ast/ast_hdr.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_hdr.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/getopt.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_lib.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_lib.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_common.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_std.h dontcare
done ${PACKAGE_ast_INCLUDE}/ast/sfio.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast_std.h implicit
make ${PACKAGE_ast_INCLUDE}/ast/ast_version.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast_version.h dontcare
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/ast.h dontcare
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/option.h dontcare
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
prev ${PACKAGE_ast_INCLUDE}/prototyped.h implicit
done ${PACKAGE_ast_INCLUDE}/ast/error.h
prev ${PACKAGE_ast_INCLUDE}/ast/ast.h implicit
done io.h
done editlib.c
prev editlib.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -DLDYNAMIC -DESH -DVSH -D_PACKAGE_ast -c editlib.c
done editlib.o generated
make vi.o
make vi.c
prev terminal.h implicit
prev edit.h implicit
prev history.h implicit
prev io.h implicit
make defs.h implicit
done defs.h dontcare virtual
done vi.c
prev vi.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -DESH -DVSH -D_PACKAGE_ast -c vi.c
done vi.o generated
make emacs.o
make emacs.c
prev edit.h implicit
prev history.h implicit
prev io.h implicit
prev defs.h implicit
done emacs.c
prev emacs.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -DESH -DVSH -D_PACKAGE_ast -c emacs.c
done emacs.o generated
make history.o
make history.c
prev national.h implicit
prev history.h implicit
prev io.h implicit
make builtins.h implicit
done builtins.h dontcare virtual
prev defs.h implicit
done history.c
prev history.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -DVSH -DESH -D_PACKAGE_ast -c history.c
done history.o generated
make edit.o
make edit.c
prev ${PACKAGE_ast_INCLUDE}/ast/error.h implicit
prev edit.h implicit
prev history.h implicit
prev io.h implicit
prev builtins.h implicit
prev terminal.h implicit
prev defs.h implicit
done edit.c
prev edit.c
exec - ${CC} ${CCFLAGS}  -I. -I${PACKAGE_ast_INCLUDE}/ast -I${PACKAGE_ast_INCLUDE} -DESH -DVSH -D_PACKAGE_ast -c edit.c
done edit.o generated
exec - ${AR} cr libedit.a editlib.o vi.o emacs.o history.o edit.o
exec - (ranlib libedit.a) >/dev/null 2>&1 || true
done libedit.a generated
done edit virtual
prev libedit.a archive
make ${PACKAGE_ast_LIB}
exec - set -
exec - if	test ! -d ${PACKAGE_ast_LIB}
exec - then	set -x && mkdir -p ${PACKAGE_ast_LIB} 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_LIB} | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_LIB} generated
make ${PACKAGE_ast_LIB}/libedit.a archive
prev libedit.a archive
exec - mv ${PACKAGE_ast_LIB}/libedit.a ${PACKAGE_ast_LIB}/libedit.a.old 2>/dev/null || true; cp libedit.a ${PACKAGE_ast_LIB}/libedit.a
exec - (ranlib ${PACKAGE_ast_LIB}/libedit.a) >/dev/null 2>&1 || true
done ${PACKAGE_ast_LIB}/libedit.a generated
make ${PACKAGE_ast_LIB}/lib
exec - set -
exec - if	test ! -d ${PACKAGE_ast_LIB}/lib
exec - then	set -x && mkdir -p ${PACKAGE_ast_LIB}/lib 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_LIB}/lib | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_LIB}/lib generated
make ${PACKAGE_ast_LIB}/lib/edit
prev edit.req
exec - mv ${PACKAGE_ast_LIB}/lib/edit ${PACKAGE_ast_LIB}/lib/edit.old 2>/dev/null || true; cp edit.req ${PACKAGE_ast_LIB}/lib/edit
done ${PACKAGE_ast_LIB}/lib/edit generated
make ${INSTALLROOT}/fun
exec - set -
exec - if	test ! -d ${INSTALLROOT}/fun
exec - then	set -x && mkdir -p ${INSTALLROOT}/fun 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${INSTALLROOT}/fun | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${INSTALLROOT}/fun generated
make ${INSTALLROOT}/fun/ie
make ie
done ie
setv mode -Dmode="+x"
exec - mv ${INSTALLROOT}/fun/ie ${INSTALLROOT}/fun/ie.old 2>/dev/null || true; cp ie ${INSTALLROOT}/fun/ie  		    		   && chmod +x ${INSTALLROOT}/fun/ie
done ${INSTALLROOT}/fun/ie generated
done install virtual
make test
done test dontcare virtual
