prev ${INSTALLROOT}/include/ast/prototyped.h implicit
done ${INSTALLROOT}/include/ast/error.h
prev ${INSTALLROOT}/include/ast/ast.h implicit
done io.h
done editlib.c
meta editlib.o %.c>%.o editlib.c editlib
prev editlib.c
exec - ${CC} ${mam_cc_FLAGS} ${CCFLAGS} -I. -I${INSTALLROOT}/include/ast -DLDYNAMIC -DESH -DVSH -D_PACKAGE_ast -c editlib.c
done editlib.o generated
