make ${INSTALLROOT}/bin
exec - set -
exec - if	test ! -d ${INSTALLROOT}/bin
exec - then	set -x && mkdir -p ${INSTALLROOT}/bin 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${INSTALLROOT}/bin | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/ed
prev ed
exec - mv ${INSTALLROOT}/bin/ed ${INSTALLROOT}/bin/ed.old 2>/dev/null || true; cp ed ${INSTALLROOT}/bin/ed
done ${INSTALLROOT}/bin/ed generated
make ${INSTALLROOT}/bin/red
prev ${INSTALLROOT}/bin/ed
exec - if	silent test -f "${INSTALLROOT}/bin/red"
exec - then	rm -f ${INSTALLROOT}/bin/red
exec - fi
exec - ln ${INSTALLROOT}/bin/ed ${INSTALLROOT}/bin/red
done ${INSTALLROOT}/bin/red generated
make ${INSTALLROOT}/bin/grep
prev grep
exec - mv ${INSTALLROOT}/bin/grep ${INSTALLROOT}/bin/grep.old 2>/dev/null || true; cp grep ${INSTALLROOT}/bin/grep
