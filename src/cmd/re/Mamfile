exec - ${CC} ${CCFLAGS}  -I${PACKAGE_ast_INCLUDE} -D_PACKAGE_ast -DUSAGE_LICENSE=\""[-author?Glenn Fowler <gsf@research.att.com>][-copyright?Copyright (c) 1995-2000 AT&T Corp.][-license?http://www.research.att.com/sw/license/ast-open.html]"\" -c ed.c
done ed.o generated
setv mam_libast ${mam_cc_L+-last}${mam_cc_L-${PACKAGE_ast_LIB}/libast.a}
exec - ${CC} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${PACKAGE_ast_LIB}}  -o ed ed.o ${mam_libast}
done ed generated
make red
prev ed
exec - if	silent test -f "red"
exec - then	rm -f red
exec - fi
exec - ln ed red
done red generated
make grep
