done ${PACKAGE_ast_INCLUDE}/error.h
prev ${PACKAGE_ast_INCLUDE}/ast.h implicit
done probe.c
prev probe.c
exec - ${CC} ${CCFLAGS}  -I${PACKAGE_ast_INCLUDE} -D_PACKAGE_ast -DUSAGE_LICENSE=\""[-author?Glenn Fowler <gsf@research.att.com>][-copyright?Copyright (c) 1989-2000 AT&T Corp.][-license?http://www.research.att.com/sw/license/ast-open.html]"\" -c probe.c
done probe.o generated
setv mam_libast ${mam_cc_L+-last}${mam_cc_L-${PACKAGE_ast_LIB}/libast.a}
exec - ${CC} ${CCFLAGS} ${LDFLAGS} ${mam_cc_L+-L${PACKAGE_ast_LIB}}  -o probe probe.o ${mam_cc_static} ${mam_libast} ${mam_cc_dynamic} ${mam_libast}
done probe generated
make ${PACKAGE_ast_LIB}/probe
exec - set -
exec - if	test ! -d ${PACKAGE_ast_LIB}/probe
exec - then	set -x && mkdir -p ${PACKAGE_ast_LIB}/probe 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${PACKAGE_ast_LIB}/probe | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${PACKAGE_ast_LIB}/probe generated
make ${PACKAGE_ast_LIB}/probe/probe
prev probe
setv mode -Dmode="u+s,+x"
exec - mv ${PACKAGE_ast_LIB}/probe/probe ${PACKAGE_ast_LIB}/probe/probe.old 2>/dev/null || true; cp probe ${PACKAGE_ast_LIB}/probe/probe  		    		   && chmod u+s,+x ${PACKAGE_ast_LIB}/probe/probe
done ${PACKAGE_ast_LIB}/probe/probe generated
make ${INSTALLROOT}/bin
exec - set -
exec - if	test ! -d ${INSTALLROOT}/bin
exec - then	set -x && mkdir -p ${INSTALLROOT}/bin 2>/dev/null && set - ||
exec - 	{
exec - 		test -d ./-p && rm -rf ./-p
exec - 		p=
exec - 		for d in `echo ${INSTALLROOT}/bin | sed -e 's%\\(.\\)/%\\1 %g'`
exec - 		do	case $p in
exec - 			"")	p=$d ;;
exec - 			*)	p=$p/$d ;;
exec - 			esac
exec - 			case $d in
exec - 			.|..)	;;
exec - 			*)	if	test ! -d $p
exec - 				then	rm -f $p
exec - 					set -x && mkdir $p && set -
exec - 				fi
exec - 				;;
exec - 			esac
exec - 		done
exec - 	} && set -x 		    		   
exec - fi
done ${INSTALLROOT}/bin generated
make ${INSTALLROOT}/bin/probe
exec - test -f ${INSTALLROOT}/bin/probe || ln -s ../lib/probe/probe ${INSTALLROOT}/bin/probe || ln ${INSTALLROOT}/lib/probe/probe ${INSTALLROOT}/bin/probe || :
done ${INSTALLROOT}/bin/probe generated
done install virtual
make test
done test dontcare virtual
