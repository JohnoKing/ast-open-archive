exec - proto -p -s -l ${INSTALLROOT}/lib/package/ast.lic '-o since=2003,author=gsf+dgk+kpv'  codex.h > 1.${COTEMP}.x
exec - if	cmp 2>/dev/null -s ${INSTALLROOT}/include/ast/codex.h 1.${COTEMP}.x
exec - then	rm -f 1.${COTEMP}.x
exec - else	mv 1.${COTEMP}.x ${INSTALLROOT}/include/ast/codex.h
exec - fi
done ${INSTALLROOT}/include/ast/codex.h generated
