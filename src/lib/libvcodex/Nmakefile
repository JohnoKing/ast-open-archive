	/* sfio */ \
	vcsfio.c \
	/* Vcdelta */ \
	vcdhdr.h vcdcache.c vcdelta.c vcdtable.c vcdtblinit.c vchamming.c \
	/* Vchuff */ \
        vchbits.c vchcode.c vchsize.c vchtrie.c \
