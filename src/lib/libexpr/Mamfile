exec - if	cmp 2>/dev/null -s ${INSTALLROOT}/include/ast/expr.h 1.${COTEMP}.x
exec - then	rm -f 1.${COTEMP}.x
exec - else	mv 1.${COTEMP}.x ${INSTALLROOT}/include/ast/expr.h
