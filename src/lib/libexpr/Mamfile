exec - {
exec - echo 'static const char* exop[] = {'
exec - echo '	"MINTOKEN",'
exec - sed -e '1,/MINTOKEN/d' -e '/MAXTOKEN/,$d' -e '/^[ 	]*#[ 	]*define[ 	][ 	]*[A-Z]/!d' -e 's/^[ 	]*#[ 	]*define[ 	]*\([A-Z0-9_]*\).*/	"\1",/'
exec - echo '	"MAXTOKEN",'
exec - echo '};'
exec - } < exparse.h > exop.h
done exop.h generated
